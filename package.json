{
  "private":true,
  "workspaces": [
    "packages/@freepress/*",
    "packages/docs"
  ],
  "scripts": {
    "bs": "lerna bootstrap",
    "cl": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "clean": "lerna clean && rm -rf node_modules",
    "dev": "./packages/docs/freepress dev",
    "generate": "./packages/docs/freepress generate",
    "generate-with-prefix": "cross-env base=/freepress && npm run generate",
    "pre-pub": "lerna publish --conventional-commits --canary",
    "pub": "lerna publish --conventional-commits",
    "serve": "./packages/docs/freepress serve",
    "build": "./packages/docs/freepress build",
    "test": "jest --config jest.config.js --collectCoverage",
    "test-u": "jest -u --collectCoverage"
  },
  "keywords": [
    "freepress",
    "react",
    "markdown"
  ],
  "engines": {
    "node": ">= 12.16.2",
    "npm": ">= 6.14.4",
    "webpack": ">= 4.0.0"
  },
  "devDependencies": {
    "babel7-jest": "^1.0.1",
    "conventional-changelog-cli": "2.0.23",
    "cross-env": "5.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "enzyme-to-json": "^3.3.5",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-serializer-enzyme": "^1.0.0",
    "lerna": "^3.11.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.6.1",
    "ts-jest": "^24.0.2",
    "typescript": "3.5.3"
  }
}
